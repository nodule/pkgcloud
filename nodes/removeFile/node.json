{
  "name": "removeFile",
  "ns": "pkgcloud",
  "title": "Remove File",
  "description": "Remove a File",
  "phrases": {
    "active": "Removing file {{input.file}} from {{input.container}}"
  },
  "ports": {
    "input": {
      "client": {
        "title": "Client",
        "type": "object",
        "required": true
      },
      "container": {
        "title": "Container",
        "type": "object",
        "required": true
      },
      "file": {
        "title": "File",
        "type": "string",
        "required": true
      }
    },
    "output": {
      "error": {
        "title": "Error",
        "type": "object"
      }
    }
  }
}
