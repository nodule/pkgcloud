{
  "name": "iriscouch",
  "ns": "pkgcloud",
  "title": "IrisCouch Credentials",
  "description": "In order to use IrisCOuch you will need to have created a valid account. ",
  "ports": {
    "input": {
      "username": {
        "title": "Username",
        "type": "string",
        "required": true
      },
      "password": {
        "title": "Password",
        "type": "password",
        "required": true
      }
    },
    "output": {
      "credentials": {
        "type": "object"
      }
    }
  }
}
